<!-- Front Ultrasonic -->
<link name="ultrasonic_front_link">
  <pose>0.09 0 0.595 0 0 0</pose>  <!-- Front center position -->
  <sensor name="ultrasonic_front" type="ray">
    <always_on>true</always_on>
    <visualize>true</visualize>
    <update_rate>5</update_rate>
    <ray>
      <!-- Same scan/range configuration as your snippet -->
    </ray>
    <plugin name="ultrasonic_sensor_front" filename="libgazebo_ros_ray_sensor.so">
      <ros>
        <remapping>~/out:=/ultrasonic/front</remapping>
      </ros>
      <output_type>sensor_msgs/Range</output_type>
      <radiation_type>ultrasound</radiation_type>
      <frame_name>ultrasonic_front_link</frame_name>
    </plugin>
  </sensor>  
</link>

